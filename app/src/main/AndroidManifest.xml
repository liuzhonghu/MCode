<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nec.myxycode">

  <uses-permission android:name="android.permission.INTERNET"/>
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity
        android:name=".MainActivity"
        android:screenOrientation="portrait">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".pop.PopShowActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".lottie.ALottieActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".lottie.SimpleLottieActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".lottie.CodeLottieActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".lottie.NetLottieActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".progressbar.ProgressBarActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".scrollnumber.ScrollNumberActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".sketch.SketchActivity"
        android:screenOrientation="portrait">
    </activity>
    <activity android:name=".matrix.MatrixActivity">
    </activity>
  </application>

</manifest>